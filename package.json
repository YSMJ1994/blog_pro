{
  "name": "soberz_website_new",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "node server/index.js",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "rebuild": "git pull && yarn run build",
    "publish2home": "scp -r ./build/* soberz@home.soberz.cn:/home/soberz/workspace/static/blog",
    "publish2tencent": "scp -r ./build/* sober@soberz.cn:/home/sober/staticManager/blog",
